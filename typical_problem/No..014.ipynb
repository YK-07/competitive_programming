    "N=int(input())\n",
    "A=list(map(int,input().split()))\n",
    "B=list(map(int,input().split()))\n",
    "A.sort()\n",
    "B.sort()\n",
    "ans=0\n",
    "for i in range(N):\n",
    "    ans+=abs(A[i]-B[i])\n",
    "print(ans)"
