   "source": [
    "N,K=map(int,input().split())\n",
    "A=list(map(int,input().split()))\n",
    "B=list(map(int,input().split()))\n",
    "for i in range(N):\n",
    "    dif=abs(A[i]-B[i])\n",
    "    K-=dif\n",
    "    if K<0:\n",
    "        break\n",
    "if K<0 or K%2==1:\n",
    "    print('No')\n",
    "else:\n",
    "    print('Yes')"
   ]
