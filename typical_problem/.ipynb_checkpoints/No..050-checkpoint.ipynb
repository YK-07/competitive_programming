   "source": [
    "N,L=map(int,input().split())\n",
    "mod=10**9+7\n",
    "step=[]\n",
    "for i in range(N+1):\n",
    "    if i<L:\n",
    "        step.append(1)\n",
    "    else:\n",
    "        step_add=step[-1]+step[-L]\n",
    "        step_add%=mod\n",
    "        step.append(step_add)\n",
    "ans=step[-1]\n",
    "#ans%=mod\n",
    "print(ans)\n"
   ]
